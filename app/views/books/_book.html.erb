<div id="<%= dom_id book %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Start date:</strong>
    <%= book.start_date %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Finish date:</strong>
    <%= book.finish_date %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Title:</strong>
    <%= book.title %>
  </div>
</div>
